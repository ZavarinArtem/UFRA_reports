<odoo>

    <record id="mrp_clear_custom_products_wizard_form" model="ir.ui.view">
        <field name="name">mrp.clear.custom.products.wizard.form</field>
        <field name="model">mrp.clear.custom.products.wizard</field>
        <field name="arch" type="xml">
            <form string="Print Options">
                <h2>
                    Current unlink already has custom products selected. Filling by bill will rewrite them.
                </h2>
                <span/>
                <h2>
                    Continue?
                </h2>
                <footer>
                    <button string="Confirm" name="confirm" type="object" class="btn-primary" />
                    <button name="cancel_button" string="Cancel" class="btn-secondary" special="cancel"/>
                </footer>
            </form>
        </field>
    </record>


</odoo>